    "babel-polyfill": "^6.23.0",
    "brain-cli": "^1.0.8-alpha.3",
    "husky": "^1.3.1",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "@types/node": "^10.0.0",
    "@types/react": "^16.9.35",
    "antd": "^2.7.4",
    "bundle-loader": "^0.5.5",
